      "instant_url": "https://search.oh64.moe/search?q={searchTerms}",
      "image_url": "https://search.oh64.moe/search?q={searchTerms}&categories=images",
      "new_tab_url": "https://search.oh64.moe/",
      "alternate_urls": [
        "https://search.oh64.moe/search?q={searchTerms}&categories=general"
      ],
      "encoding": "UTF-8",
      "prepopulated_id": 1,
      "is_default": true
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }

