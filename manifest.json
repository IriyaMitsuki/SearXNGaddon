{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "1.6",
  "default_locale": "en",
  "icons": {
    "128": "icon.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "SearXNG",
      "keyword": "searxng",
      "search_url": "https://search.oh64.moe/search?q={searchTerms}",
      "suggest_url": "https://search.oh64.moe/autocompleter?q={searchTerms}",
